<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quygt.dkcs.dao.UserLoginLogDao">
    <insert id="insert" parameterType="UserLoginLog" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO fdcs_user_login_log
        (
          userId,userName,realName,state,isApp,version,deviceInfo,createTime
        )
        VALUES
        (
          #{userId},#{userName},#{realName},#{state},#{isApp},#{version},#{deviceInfo},#{createTime}
        )
    </insert>

    <select id="getlist" resultType="UserLoginLog">
        SELECT * FROM fdcs_user_login_log
        <trim prefix="WHERE" prefixOverrides="AND">
            1=1
            <if test="userId>0 ">AND userId=#{userId}</if>
            <if test="userName!= null ">AND userName=#{userName}</if>
            <if test="realName!= null ">AND realName=#{realName}</if>
            <if test="isApp>0 ">AND isApp=#{isApp}</if>
            <if test="version!= null ">AND version=#{version}</if>
            <if test="state>0 ">AND state=#{state}</if>
            <if test="deviceInfo!=null ">AND deviceInfo=#{deviceInfo}</if>
            <if test="createTime!=null ">AND createTime=#{createTime}</if>
        </trim>
        ORDER BY id DESC
    </select>
</mapper>